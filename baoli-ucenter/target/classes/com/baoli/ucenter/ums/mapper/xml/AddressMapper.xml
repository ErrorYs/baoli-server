<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baoli.ucenter.ums.mapper.AddressMapper">

    <update id="changeDft" parameterType="string">
        update ums_address set dft = 0 where dft = 1 and member_id =  #{memberId}
    </update>
</mapper>
